module sonic-portchannel-annot {

    yang-version "1.1";

    namespace "http://openconfig.net/yang/annotation";
    prefix "spc-annot";

    import sonic-portchannel { prefix spc; }

    deviation /spc:sonic-portchannel-state/spc:LAG_TABLE {
      deviate add {
            sonic-ext:db-name "APPL_DB";
      }
    }


    deviation /spc:sonic-portchannel-state/spc:LAG_MEMBER_TABLE {
      deviate add {
        sonic-ext:db-name "APPL_DB";
      }
    }

}

